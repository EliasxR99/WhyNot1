// Counter Classic
section.section.section-fluid.bg-default
	.container-fluid
		+parallax({
			img: 'bg-counter-2',
			innerClass: 'section-xl'
		})
			.container
				.row.row-50.justify-content-center.border-classic
					.col-sm-6.col-md-5.col-lg-3
						+counter-classic({
							number: '12',
							title: 'Awards won'
						})
					.col-sm-6.col-md-5.col-lg-3
						+counter-classic({
							number: '194',
							title: 'Apps developed'
						})
					.col-sm-6.col-md-5.col-lg-3
						+counter-classic({
							number: '2',
							symbol: 'k',
							title: 'Satisfied customers'
						})
					.col-sm-6.col-md-5.col-lg-3
						+counter-classic({
							number: '25',
							title: 'team members'
						})
='\n'